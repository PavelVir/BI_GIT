			И ИНАГРО_УГПРРаботы.Версия = ИНАГРО_РаспределениеРаботСрезПоследних.Версия
ГДЕ
	ИНАГРО_УГПРРаботы.ЭтоРабота
{ГДЕ
	(ИНАГРО_УГПРРаботы.Версия = &amp;Версия),
	ИНАГРО_УГПРРаботы.Конструктив.*,
	ИНАГРО_УГПРРаботы.Организация.*,
	ИНАГРО_УГПРРаботы.ВидРаботы.*,
	ИНАГРО_УГПРРаботы.ЭтоРабота,
	(ЕСТЬNULL(ИНАГРО_РаспределениеРаботСрезПоследних.ОбъемСубподряда, 0) &gt; 0) КАК Субподряд,
	(ИНАГРО_УГПРРаботы.Проект = &amp;Проект),
	(ИНАГРО_УГПРРаботы.Версия.ВидВерсии = ЗНАЧЕНИЕ(Перечисление.ИНАГРО_ВидВерсии.Рабочая)) КАК ТолькоРабочие}

