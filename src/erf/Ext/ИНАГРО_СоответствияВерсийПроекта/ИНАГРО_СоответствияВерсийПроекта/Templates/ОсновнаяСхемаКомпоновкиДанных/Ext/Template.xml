	Проект.*,
	Версия.*,
	КлючСтроки,
	Объем,
	ДлительностьЧасы,
	ДлительностьДни,
	НомерВСтруктуре,
	Конструктив.*,
	ВидРаботы.*,
	КлючСвязи,
	ЭтоРабота,
	Субподряд,
	НакладныеРасходы,
	КодWBS}
ИЗ
	РегистрСведений.ИНАГРО_УГПРРаботы КАК ИНАГРО_УГПРРаботы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ИНАГРО_РаспределениеРабот.СрезПоследних КАК ИНАГРО_РаспределениеРаботСрезПоследних
