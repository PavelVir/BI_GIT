	<dataSet xsi:type="DataSetQuery">
		<name>ТаблицаИерархии</name>
		<field xsi:type="DataSetFieldField">
			<dataPath>КлючСвязи</dataPath>
			<field>КлючСвязи</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Версия</dataPath>
			<field>Версия</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
			</role>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВидРаботы</dataPath>
			<field>ВидРаботы</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НомерВСтруктуре</dataPath>
			<field>НомерВСтруктуре</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КодWBS</dataPath>
			<field>КодWBS</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ШифрПозицииНорматива</dataPath>
			<field>ШифрПозицииНорматива</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Шифр позиции норматива</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>КлючСтроки</dataPath>
			<field>КлючСтроки</field>
			<role>
				<dcscom:dimension>true</dcscom:dimension>
				<dcscom:parentDimension>КлючСвязи</dcscom:parentDimension>
			</role>
			<presentationExpression>ВидРаботы.ШифрПозицииНорматива</presentationExpression>
			<appearance>
				<dcscor:item xsi:type="dcsset:SettingsParameterValue">
					<dcscor:parameter>МаксимальнаяШирина</dcscor:parameter>
					<dcscor:value xsi:type="xs:decimal">0.01</dcscor:value>
				</dcscor:item>
			</appearance>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ВидОбъекта</dataPath>
			<field>ВидОбъекта</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Вид объекта</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ИНАГРО_УГПРРаботы.Версия КАК Версия,
	ИНАГРО_УГПРРаботы.КлючСтроки КАК КлючСтроки,
	ИНАГРО_УГПРРаботы.КлючСвязи КАК КлючСвязи,
	ИНАГРО_УГПРРаботы.ВидРаботы КАК ВидРаботы,
	ИНАГРО_УГПРРаботы.КодWBS КАК КодWBS,
	ИНАГРО_УГПРРаботы.НомерВСтруктуре КАК НомерВСтруктуре,
	ВЫБОР
		КОГДА ИНАГРО_УГПРРаботы.ЭтоРабота
			ТОГДА ИНАГРО_УГПРРаботы.ВидРаботы.ШифрПозицииНорматива
		ИНАЧЕ ИНАГРО_УГПРРаботы.КороткоеИмя
	КОНЕЦ КАК ШифрПозицииНорматива,
	ВЫБОР
		КОГДА ИНАГРО_УГПРРаботы.ЭтоРабота
			ТОГДА ""
		ИНАЧЕ ВЫБОР
				КОГДА НЕ ИНАГРО_УГПРРаботы.ВидРаботы.ОбъектПроекта = ЗНАЧЕНИЕ(Справочник.ИНАГРО_СтроительныеОбъекты.ПустаяСсылка)
					ТОГДА "Строительный объект"
				КОГДА НЕ ИНАГРО_УГПРРаботы.ВидРаботы.ЭлементОбъекта = ЗНАЧЕНИЕ(Справочник.ИНАГРО_ЭлементыОбъектовКлас.ПустаяСсылка)
					ТОГДА "Элемент объекта строительства 1 уровня"
				КОГДА НЕ ИНАГРО_УГПРРаботы.ВидРаботы.ЭлементОбъекта2 = ЗНАЧЕНИЕ(Справочник.ИНАГРО_ЭлементыОбъектовКлас2.ПустаяСсылка)
					ТОГДА "Элемент объекта строительства 2 уровня"
				КОГДА НЕ ИНАГРО_УГПРРаботы.ВидРаботы.Конструктив = ЗНАЧЕНИЕ(Справочник.ИНАГРО_Конструктивы.ПустаяСсылка)
					ТОГДА "Конструктив (класс)"
				КОГДА НЕ ИНАГРО_УГПРРаботы.ВидРаботы.ПодКонструктив = ЗНАЧЕНИЕ(Справочник.ИНАГРО_ПодКонструктив.ПустаяСсылка)
					ТОГДА "ПодКонструктив (класс)"
				ИНАЧЕ ""
			КОНЕЦ
	КОНЕЦ КАК ВидОбъекта
{ВЫБРАТЬ
	Версия.*,
	КлючСтроки,
	КлючСвязи,
	ВидРаботы.*,
	КодWBS,
	НомерВСтруктуре,
	ШифрПозицииНорматива}
ИЗ
	РегистрСведений.ИНАГРО_УГПРРаботы КАК ИНАГРО_УГПРРаботы
ГДЕ
	ИНАГРО_УГПРРаботы.Версия.ВидВерсии = ЗНАЧЕНИЕ(Перечисление.ИНАГРО_ВидВерсии.Рабочая)
{ГДЕ
	ИНАГРО_УГПРРаботы.Версия.*}</query>
	</dataSet>
