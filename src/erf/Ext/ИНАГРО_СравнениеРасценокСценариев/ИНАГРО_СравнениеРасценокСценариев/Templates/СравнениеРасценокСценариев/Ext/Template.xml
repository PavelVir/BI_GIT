	ВТРаботы.Проект,
	ВТРаботы.НомерВСтруктуре,
	ВТРаботы.Объем КАК Объем,
	ЕСТЬNULL(ВТРаботы.ВидРаботы, ЗНАЧЕНИЕ(Справочник.ИНАГРО_ВидыРаботПроект.ПустаяСсылка)) КАК ВидРаботы,
	ВТРаботы.КлючСтроки,
	ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Исполнитель, ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка)) КАК Исполнитель,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.СтатьяРасходов,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Ресурс,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.ЕдиницаИзмерения,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Цена КАК РасходыНаЕдиницу,
	ВЫБОР
		КОГДА ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Субподряд, ЛОЖЬ)
			ТОГДА ИНАГРО_РасценкаБюджетаПроектаРаботы.СтоимостьСубподряда
		ИНАЧЕ 0
	КОНЕЦ КАК СтоимостьСубподряда,
	ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Субподряд, ЛОЖЬ) КАК Субподряд,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Количество * ИНАГРО_РасценкаБюджетаПроектаРесурсы.Цена КАК ОбщиеРасходы,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТРаботы.Объем, 0) = 0
			ТОГДА 0
		ИНАЧЕ ИНАГРО_РасценкаБюджетаПроектаРесурсы.Количество * ИНАГРО_РасценкаБюджетаПроектаРесурсы.Цена / ВТРаботы.Объем
	КОНЕЦ КАК РасходыЗаЕдиницуОбъема,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТРаботы.Объем, 0) = 0
			ТОГДА 0
		ИНАЧЕ ВТРаботы.Сумма / ВТРаботы.Объем
	КОНЕЦ КАК ДоходыЗаЕдиницу,
	ЕСТЬNULL(ВТРаботы.Сумма, 0) КАК ОбщиеДоходы,
	ЕСТЬNULL(ВТРаботы.Сумма, 0) - ВЫБОР
		КОГДА ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Субподряд, ЛОЖЬ)
			ТОГДА ИНАГРО_РасценкаБюджетаПроектаРаботы.СтоимостьСубподряда
		ИНАЧЕ ИНАГРО_РасценкаБюджетаПроектаРесурсы.Количество * ИНАГРО_РасценкаБюджетаПроектаРесурсы.Цена
	КОНЕЦ КАК Маржа,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.ПроцентНаценки КАК ПроцентНаценкиСценарий,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.СуммаСНаценкой КАК ОбщиеРасходыСНаценкойСценарий,
	ВЫБОР
		КОГДА ЕСТЬNULL(ВТРаботы.Объем, 0) = 0
			ТОГДА 0
		ИНАЧЕ ИНАГРО_РасценкаБюджетаПроектаРесурсы.СуммаСНаценкой / ВТРаботы.Объем
	КОНЕЦ КАК РасходыСНаценкойЗаЕдиницуОбъема,
	ВЫБОР
		КОГДА ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий = &amp;Сценарий1
			ТОГДА ИНАГРО_РасценкаБюджетаПроектаРесурсы.СуммаСНаценкой
		ИНАЧЕ 0
	КОНЕЦ КАК ОбщиеРасходыСНаценкойСценарий1,
	ВЫБОР
		КОГДА ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий = &amp;Сценарий2
			ТОГДА ИНАГРО_РасценкаБюджетаПроектаРесурсы.СуммаСНаценкой
		ИНАЧЕ 0
	КОНЕЦ КАК ОбщиеРасходыСНаценкойСценарий2,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Количество КАК КоличествоСценарий
ИЗ
	ВТРаботы КАК ВТРаботы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ИНАГРО_РасценкаБюджетаПроектаРаботы КАК ИНАГРО_РасценкаБюджетаПроектаРаботы
			ПОЛНОЕ СОЕДИНЕНИЕ РегистрНакопления.ИНАГРО_РасценкаБюджетаПроектаРесурсы КАК ИНАГРО_РасценкаБюджетаПроектаРесурсы
			ПО (ИНАГРО_РасценкаБюджетаПроектаРесурсы.КлючСтроки = ИНАГРО_РасценкаБюджетаПроектаРаботы.КлючСтроки)
				И (ИНАГРО_РасценкаБюджетаПроектаРесурсы.Версия = ИНАГРО_РасценкаБюджетаПроектаРаботы.Версия)
				И (ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий = ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий)
		ПО (ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.КлючСтроки, ИНАГРО_РасценкаБюджетаПроектаРесурсы.КлючСтроки) = ВТРаботы.КлючСтроки)
			И (ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Версия, ИНАГРО_РасценкаБюджетаПроектаРесурсы.Версия) = ВТРаботы.Версия)
ГДЕ
	(ВТРаботы.Версия = &amp;Версия1
			ИЛИ ВТРаботы.Версия = &amp;Версия2)
	И (ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий, ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий) = &amp;Сценарий1
			ИЛИ ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий, ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий) = &amp;Сценарий2
			ИЛИ ЕСТЬNULL(ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий, ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий) ЕСТЬ NULL)</query>
