					<v8:lang>ru</v8:lang>
					<v8:content>Сумма расхода</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Версия,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.КлючСтроки,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий,
	ЗНАЧЕНИЕ(Справочник.СМ_Ресурсы.ПустаяСсылка) КАК Ресурс,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.СтатьяРасходов КАК СтатьяРасходов,
	ВЫБОР
		КОГДА ИНАГРО_РасценкаБюджетаПроектаРаботы.СтоимостьСубподрядаОборот = 0
			ТОГДА ИНАГРО_РасценкаБюджетаПроектаРаботы.СметнаяСтоимостьОборот
		ИНАЧЕ ИНАГРО_РасценкаБюджетаПроектаРаботы.СтоимостьСубподрядаОборот + ИНАГРО_РасценкаБюджетаПроектаРаботы.НакладныеРасходыРаспредОборот
	КОНЕЦ КАК СуммаСубподряда,
	0 КАК СуммаЗатрат,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.НакладныеРасходыРаспредОборот,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.СметнаяПрибыльОборот,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.НакладныеРасходыОборот,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.ИндексацияОборот,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.СметнаяСтоимостьОборот,
	ИСТИНА КАК ЭтоРабота
ПОМЕСТИТЬ ВТСуммыРасходов_Предгруппировка
ИЗ
	РегистрНакопления.ИНАГРО_РасценкаБюджетаПроектаРаботы.Обороты(
			,
			,
			Авто,
			Сценарий = &amp;Сценарий
				И Версия = &amp;Версия) КАК ИНАГРО_РасценкаБюджетаПроектаРаботы

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Версия,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.КлючСтроки,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Сценарий,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.Ресурс,
	ИНАГРО_РасценкаБюджетаПроектаРесурсы.СтатьяРасходов,
	0,
	ВЫБОР
		КОГДА ИНАГРО_РасценкаБюджетаПроектаРесурсы.Субподряд
			ТОГДА 0
		ИНАЧЕ ИНАГРО_РасценкаБюджетаПроектаРесурсы.СуммаСНаценкойОборот
	КОНЕЦ,
	0,
	0,
	0,
	0,
	0,
	ЛОЖЬ
ИЗ
	РегистрНакопления.ИНАГРО_РасценкаБюджетаПроектаРесурсы.Обороты(
			,
			,
			Авто,
			Сценарий = &amp;Сценарий
				И Версия = &amp;Версия) КАК ИНАГРО_РасценкаБюджетаПроектаРесурсы
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТСуммыРасходов_Предгруппировка.Версия,
	ВТСуммыРасходов_Предгруппировка.КлючСтроки,
	ВТСуммыРасходов_Предгруппировка.Сценарий,
	ВТСуммыРасходов_Предгруппировка.Ресурс,
	ВТСуммыРасходов_Предгруппировка.СтатьяРасходов,
	СУММА(ВТСуммыРасходов_Предгруппировка.СуммаСубподряда) КАК СуммаСубподряда,
	СУММА(ВТСуммыРасходов_Предгруппировка.СуммаЗатрат) КАК СуммаЗатрат,
	СУММА(ВТСуммыРасходов_Предгруппировка.НакладныеРасходыРаспредОборот) КАК НакладныеРасходыРаспредОборот,
	СУММА(ВТСуммыРасходов_Предгруппировка.СметнаяПрибыльОборот) КАК СметнаяПрибыльОборот,
	СУММА(ВТСуммыРасходов_Предгруппировка.НакладныеРасходыОборот) КАК НакладныеРасходыОборот,
	СУММА(ВТСуммыРасходов_Предгруппировка.ИндексацияОборот) КАК ИндексацияОборот,
	ВТСуммыРасходов_Предгруппировка.СметнаяСтоимостьОборот,
	ВТСуммыРасходов_Предгруппировка.ЭтоРабота
ПОМЕСТИТЬ ВТ_СуммыРасхода_Сгруппированы
ИЗ
	ВТСуммыРасходов_Предгруппировка КАК ВТСуммыРасходов_Предгруппировка

СГРУППИРОВАТЬ ПО
	ВТСуммыРасходов_Предгруппировка.Сценарий,
	ВТСуммыРасходов_Предгруппировка.Версия,
	ВТСуммыРасходов_Предгруппировка.КлючСтроки,
	ВТСуммыРасходов_Предгруппировка.Ресурс,
	ВТСуммыРасходов_Предгруппировка.СтатьяРасходов,
	ВТСуммыРасходов_Предгруппировка.СметнаяСтоимостьОборот,
	ВТСуммыРасходов_Предгруппировка.ЭтоРабота
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Версия,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.КлючСтроки,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Исполнитель,
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Субподряд
ПОМЕСТИТЬ ВТ_РасценкаРаботы
ИЗ
	РегистрНакопления.ИНАГРО_РасценкаБюджетаПроектаРаботы КАК ИНАГРО_РасценкаБюджетаПроектаРаботы
ГДЕ
	ИНАГРО_РасценкаБюджетаПроектаРаботы.Версия = &amp;Версия
	И ИНАГРО_РасценкаБюджетаПроектаРаботы.Сценарий = &amp;Сценарий
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_РасценкаРаботы.Версия,
	ВТ_РасценкаРаботы.КлючСтроки,
	ВТ_РасценкаРаботы.Сценарий,
	ВТ_РасценкаРаботы.Исполнитель,
	ВТ_РасценкаРаботы.Субподряд,
	ВТСуммыРасходов.Ресурс,
	ВТСуммыРасходов.СтатьяРасходов,
	ВЫБОР
		КОГДА ВТ_РасценкаРаботы.Субподряд
				И ВТСуммыРасходов.ЭтоРабота
			ТОГДА ЕСТЬNULL(ВТСуммыРасходов.СуммаСубподряда, 0)
		ИНАЧЕ ЕСТЬNULL(ВТСуммыРасходов.СуммаЗатрат + ВТСуммыРасходов.НакладныеРасходыОборот + ВТСуммыРасходов.НакладныеРасходыРаспредОборот + ВТСуммыРасходов.СметнаяПрибыльОборот + ВТСуммыРасходов.ИндексацияОборот, 0)
	КОНЕЦ КАК СуммаРасхода
ИЗ
	ВТ_РасценкаРаботы КАК ВТ_РасценкаРаботы
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СуммыРасхода_Сгруппированы КАК ВТСуммыРасходов
		ПО ВТ_РасценкаРаботы.КлючСтроки = ВТСуммыРасходов.КлючСтроки
			И ВТ_РасценкаРаботы.Сценарий = ВТСуммыРасходов.Сценарий
			И ВТ_РасценкаРаботы.Версия = ВТСуммыРасходов.Версия</query>
	</dataSet>
